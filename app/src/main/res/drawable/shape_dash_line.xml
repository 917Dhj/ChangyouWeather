<?xml version="1.0" encoding="utf-8"?>
<!--点分割线-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <size android:height="1dp" />
    <stroke
        android:dashGap="8dp"
        android:dashWidth="4dp"
        android:width="1dp"
        android:color="@color/line" />
</shape>